# Mailchimp API Configuration
# Copy this file to .env and fill in your actual values

# Your Mailchimp API Key
# Found at: Account > Extras > API keys
MAILCHIMP_API_KEY=your_api_key_here

# Your Mailchimp Audience (List) ID
# Found at: Audience > Settings > Audience name and defaults
MAILCHIMP_AUDIENCE_ID=your_audience_id_here

# Your Mailchimp Server Prefix (e.g., us1, us6, us19)
# This is the part before '.api.mailchimp.com' in your API endpoint
# Found in your API key or account settings
MAILCHIMP_SERVER_PREFIX=us1
